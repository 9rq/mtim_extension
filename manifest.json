{
    "manifest_version": 2,

    "name": "myTIM extension",
    "description": "for myTIM",
    "version": "1.1",

    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },

    "content_scripts": [
        {
            "js": [ "punchClock_mod.js" ],
            "matches": [ "https://whm.accenture.com/mytim/secure/punchClock" ]
        },
        {
            "js": ["confirm_mod.js"],
            "matches": [ "https://whm.accenture.com/mytim/secure/punchClock/confirm" ]
        }
    ]
}
